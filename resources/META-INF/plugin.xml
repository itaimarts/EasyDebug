<idea-plugin>
  <id>org.jetbrains.plugins.sample.ToolWindow</id>
  <name>Tool Window Demo</name>
  <description>This sample plugin illustrates how to create your custom tool window.</description>
  <version>2.0</version>
  <vendor>JetBrains</vendor>

  <idea-version since-build="173.0"/>

  <extensions defaultExtensionNs="com.intellij">
    <toolWindow id="RP Debug" icon="/myToolWindow/debugger-13.png" anchor="right" factoryClass="com.dell.easydebug.ui.ToolWindowFactoryImpl"/>

  </extensions>

</idea-plugin>